{
  "name": "eggshell-official-website",
  "version": "0.0.0",
  "description": "eggshell 个人官网",
  "author": "Magic-Jun-K",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter frontend dev",
    "build": "pnpm --filter frontend build",
    "preview": "pnpm --filter frontend preview",
    "lint": "oxlint .",
    "lint:fix": "oxlint . --fix",
    "type-check": "pnpm --filter frontend type-check",
    "clean": "pnpm -r exec rm -rf node_modules dist .turbo",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "lint-staged": "lint-staged",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/cz-commitlint": "^20.0.0",
    "@types/node": "^24.5.2",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.1",
    "oxlint": "^1.18.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "~5.9.2"
  },
  "packageManager": "pnpm@10.17.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
